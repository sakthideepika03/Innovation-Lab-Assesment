// Object Destructuring
let student = {
  name: 'John Doe',
  age: 20,
  course: 'JavaScript'
};

// Extract properties from objects into variables
let { name, age } = student;
console.log(name); // Output: "John Doe"
console.log(age); // Output: 20

// Destructuring with default values
let { courseName = 'Default Course' } = { courseName: undefined };
console.log(courseName); // Output: "Default Course"

let { course: courseTitle = 'Default Course' } = student;
console.log(courseTitle); // Output: "JavaScript"

// Destructuring with nested objects
let nestedStudent = {
  name: 'Jane Doe',
  age: 22,
  address: {
    street: '123 Main St',
    city: 'Anytown'
  }
};

let { address: { street, city } } = nestedStudent;
console.log(street); // Output: "123 Main St"
console.log(city); // Output: "Anytown"

// Spread Operator
let clonedStudent = { ...student };
console.log(clonedStudent); // Output: { name: "John Doe", age: 20, course: "JavaScript" }

// Merge objects
let additionalInfo = {
  grade: 'A',
  attendance: 'Regular'
};

let mergedStudent = { ...student, ...additionalInfo };
console.log(mergedStudent); 
// Output: { name: "John Doe", age: 20, course: "JavaScript", grade: "A", attendance: "Regular" }
